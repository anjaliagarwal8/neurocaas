{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": "s3:GetBucketLocation",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor3",
      "Effect": "Allow",
      "Action": "s3:HeadBucket",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    },
    {
      "Sid": "ListBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::dlc-ncap-web"
      ],
      "Condition": {
        "StringEquals": {
          "s3:prefix": [
            "",
            "brunolab/",
            "brunolab/inputs",
            "brunolab/results",
            "logs",
            "brunolab/submissions",
            "brunolab/configs",
            "brunolab/inputs/",
            "brunolab/results/",
            "brunolab/submissions/",
            "brunolab/configs/"
          ],
          "s3:delimiter": [
            "/"
          ]
        }
      }
    },
    {
      "Sid": "ListSubBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::dlc-ncap-web"
      ],
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "brunolab/inputs/*",
            "brunolab/results/*",
            "brunolab/configs/*",
            "brunolab/submissions/*"
          ]
        }
      }
    },
    {
      "Sid": "Inputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::dlc-ncap-web/brunolab/inputs/*",
        "arn:aws:s3:::dlc-ncap-web/brunolab/configs/*",
        "arn:aws:s3:::dlc-ncap-web/brunolab/submissions/*"
      ]
    },
    {
      "Sid": "Outputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::dlc-ncap-web/brunolab/results/*",
        "arn:aws:s3:::dlc-ncap-web/brunolab/configs/*"
      ]
    }
  ]
}