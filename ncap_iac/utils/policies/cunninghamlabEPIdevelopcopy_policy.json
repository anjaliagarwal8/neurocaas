{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": "s3:GetBucketLocation",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor3",
      "Effect": "Allow",
      "Action": "s3:HeadBucket",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    },
    {
      "Sid": "ListBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::epi-ncap-stable-copy"
      ],
      "Condition": {
        "StringEquals": {
          "s3:prefix": [
            "",
            "cunninghamlabEPIdevelopcopy/",
            "cunninghamlabEPIdevelopcopy/inputs",
            "cunninghamlabEPIdevelopcopy/results",
            "logs",
            "cunninghamlabEPIdevelopcopy/submissions",
            "cunninghamlabEPIdevelopcopy/configs",
            "cunninghamlabEPIdevelopcopy/inputs/",
            "cunninghamlabEPIdevelopcopy/results/",
            "cunninghamlabEPIdevelopcopy/submissions/",
            "cunninghamlabEPIdevelopcopy/configs/"
          ],
          "s3:delimiter": [
            "/"
          ]
        }
      }
    },
    {
      "Sid": "ListSubBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::epi-ncap-stable-copy"
      ],
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "cunninghamlabEPIdevelopcopy/inputs/*",
            "cunninghamlabEPIdevelopcopy/results/*",
            "cunninghamlabEPIdevelopcopy/configs/*",
            "cunninghamlabEPIdevelopcopy/submissions/*"
          ]
        }
      }
    },
    {
      "Sid": "Inputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::epi-ncap-stable-copy/cunninghamlabEPIdevelopcopy/inputs/*",
        "arn:aws:s3:::epi-ncap-stable-copy/cunninghamlabEPIdevelopcopy/configs/*",
        "arn:aws:s3:::epi-ncap-stable-copy/cunninghamlabEPIdevelopcopy/submissions/*"
      ]
    },
    {
      "Sid": "Outputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::epi-ncap-stable-copy/cunninghamlabEPIdevelopcopy/results/*",
        "arn:aws:s3:::epi-ncap-stable-copy/cunninghamlabEPIdevelopcopy/configs/*"
      ]
    }
  ]
}