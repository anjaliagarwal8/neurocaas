{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": "s3:GetBucketLocation",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor3",
      "Effect": "Allow",
      "Action": "s3:HeadBucket",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    },
    {
      "Sid": "ListHomeBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::frontendtestgroup2"
      ],
      "Condition": {
        "StringEquals": {
          "s3:prefix": [
            "",
            "inputs",
            "results",
            "logs",
            "inputs/",
            "results/",
            "logs/"
          ],
          "s3:delimiter": [
            "/"
          ]
        }
      }
    },
    {
      "Sid": "ListHomeSubBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::frontendtestgroup2"
      ],
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "",
            "inputs/*",
            "results/*",
            "logs/*"
          ]
        }
      }
    },
    {
      "Sid": "Homeinputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::frontendtestgroup2/inputs/*"
      ]
    },
    {
      "Sid": "Homeoutputfolderget",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::frontendtestgroup2/results/*"
      ]
    }
  ]
}